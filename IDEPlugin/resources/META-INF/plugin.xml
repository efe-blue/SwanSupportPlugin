<idea-plugin>
  <id>com.your.company.unique.plugin.id</id>
  <type>swan support</type>
  <version>1.0</version>
  <vendor email="support@yourcompany.com" url="http://www.yourcompany.com">YourCompany</vendor>

  <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

  <change-notes><![CDATA[
      Add change notes here.<br>
      <em>most HTML tags may be used</em>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <depends>com.intellij.modules.xml</depends>
  <depends>com.intellij.modules.lang</depends>
  <depends>JavaScript</depends>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <fileTypeFactory implementation="com.apkfuns.swan.factory.SwanFileTypeFactory"/>
    <xml.tagNameProvider implementation="com.apkfuns.swan.provider.SwanTagNameProvider"/>
    <xml.elementDescriptorProvider implementation="com.apkfuns.swan.provider.SwanTagNameProvider"/>
    <xml.attributeDescriptorsProvider implementation="com.apkfuns.swan.attributes.SwanAttrDescriptorProvider"/>
    <completion.contributor language="HTML" implementationClass="com.apkfuns.swan.factory.SwanCompletionContributor"/>
    <defaultLiveTemplatesProvider implementation="com.apkfuns.swan.provider.SwanTemplatesProvider"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>